<?xml version="1.0" encoding="UTF-8"?>
<mvc-config>
    <action name="login" class="cn.xxyangyoulin.action.LoginAction" method="login">
        <result name="success" type="redirect">/index.jsp</result>
        <result name="fail">/login.jsp</result>
    </action>

    <action name="register" class="cn.xxyangyoulin.action.RegisterAction" method="register">
